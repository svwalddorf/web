{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Seite",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subPages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page",
      "mappedBy": "parentPage"
    },
    "parentPage": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page.page",
      "inversedBy": "subPages"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "contents": {
      "type": "dynamiczone",
      "components": [
        "block.carousel",
        "block.rich-text",
        "shared.spacing"
      ]
    },
    "leftContent": {
      "type": "dynamiczone",
      "components": [
        "block.carousel",
        "block.rich-text",
        "shared.spacing"
      ]
    },
    "rightContent": {
      "type": "dynamiczone",
      "components": [
        "block.carousel",
        "block.rich-text",
        "shared.spacing"
      ]
    },
    "inContainer": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
